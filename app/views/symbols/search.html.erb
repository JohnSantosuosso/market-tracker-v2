<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Symbol</th>
        <th>Name</th>
        <th>Price</th>
        <th></th> <!-- Empty column for the select button -->
      </tr>
    </thead>
    <tbody>
      <% @symbols.each do |symbol| %>
        <tr>
          <td><%= symbol.symbol %></td>
          <td><%= symbol.name %></td>
          <td><%= symbol.exchange %></td>
          <td>
            <button class="btn btn-primary">Select</button>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

